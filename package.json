{
  "name": "v-tube",
  "version": "1.0.0",
  "description": "A video streaming site",
  "main": "index.js",
  "scripts": {
    "make-migration": "npx knex migrate:make session --knexfile ./config/knexfile.ts --env development",
    "migrate:up": "npx knex migrate:latest --knexfile ./config/knexfile.ts --env development",
    "migrate:down": "npx knex migrate:down --knexfile ./config/knexfile.ts --env development",
    "make-seed": "npx knex seed:make new-seed --knexfile ./src/Config/knexfile.ts --env development",
    "seed": "npx knex seed:run --knexfile=../Config/knexfile.ts --env development",
    "dev": "ts-node-dev --respawn --transpile-only ./src/app.ts "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abigail-cloud/v-tube.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Abigail-cloud/v-tube/issues"
  },
  "homepage": "https://github.com/Abigail-cloud/v-tube#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dayjs": "^1.11.8",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "objection": "^3.0.1",
    "pg": "^8.11.0",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^20.2.5",
    "@types/pino": "^7.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
